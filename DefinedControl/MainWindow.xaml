<Window x:Class="DefinedControl.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Button Style="{StaticResource MyButtonStyle}"  Margin="10,20,10,10" Content="Start"/>
            <CheckBox Style="{StaticResource MyCheckBoxStyle}" Width="60" Height="20" Margin="10,20,10,10"/>
            <TextBox Style="{StaticResource MyTextBoxStyle}" Width="200" Height="30" Margin="10,20,10,10" Text="Hello WPF" />
        </StackPanel>
    </Grid>
</Window>
